{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@now/static" },
    { "src": "functions/daily.js", "use": "@now/node" },
    { "src": "functions/weekly.js", "use": "@now/node" },
    { "src": "functions/monthly.js", "use": "@now/node" }
  ],
  "env": {
    "APPLICATION_CONSUMER_KEY": "@application_consumer_key",
    "APPLICATION_SECRET": "@application_secret",
    "USER_ACCESS_TOKEN": "@user_access_token",
    "USER_SECRET": "@user_secret"
  }
}
